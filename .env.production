# Configuração para Produção

# Database
DATABASE_URL="file:./prod.db"

# Server
PORT=3000
NODE_ENV=production

# JWT Secret (ALTERE PARA UMA CHAVE SEGURA EM PRODUÇÃO)
JWT_SECRET=your_super_secure_jwt_secret_key_here_change_this

# CORS (Configure conforme necessário)
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logs
LOG_LEVEL=info

